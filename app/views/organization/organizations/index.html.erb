<%= link_to 'New', new_organization_path if can? :create, Organization %>
<% if @organizations.present? %>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Phone numebr</th>
        <th>URL</th>
        <th>Status</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'table', locals: {organizations: @organizations} %>
    </tbody>
  </table>
<% else %>
  No records found.
<% end %>
<%= link_to 'Back', root_path %>